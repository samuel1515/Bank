  <script>
    function handleWithdrawal() {
      var withdrawalAmountInput = document.getElementById('withdrawal-amount');
      var balanceElement = document.getElementById('balance');
      
      var withdrawalAmount = parseFloat(withdrawalAmountInput.value);
      var currentBalance = parseFloat(balanceElement.textContent.replace('Saldo: $', ''));
      
      if (withdrawalAmount <= currentBalance) {
        balanceElement.textContent = 'Enviado';
        withdrawalAmountInput.value = '';
      }
    }
  </script>
